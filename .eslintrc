{
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"extends": ["standard", "eslint-config-prettier"],
	"rules": {
		"valid-typeof": "error",
		"no-unsafe-negation": "error",
		"no-unused-vars": "error",
		"no-unexpected-multiline": "error",
		"no-undef": "error"
	},
	"env": {
		"browser": true,
		"jest": true
	}
}
